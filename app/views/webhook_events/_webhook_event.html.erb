<div id="<%= dom_id(webhook_event) %>" data-controller="beautifyjson">
  <span data-beautifyjson-target="payloadJson" data-actual-json="<%= webhook_event.payload.to_json %>"/>
  <div>
    <%= webhook_event.payload %>
  </div>
  <div>
    <%= time_tag webhook_event.created_at, "data-local": "time-or-date" %>
  </div>
</div>